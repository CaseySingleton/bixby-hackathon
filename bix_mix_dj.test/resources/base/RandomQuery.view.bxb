result-view {
  match { Tracks (tracks) }
  render {
     list-of (tracks) { 
      where-each (item) {
        cell-area { 
          slot1 {
            image {
              url ("#{value(item.trackAlbumImage[0])}")
              shape (Square)
            }
          }
          slot2 {
            content {
              order (PrimarySecondary)
            primary ("#{value(item.trackNames[0])}")
              secondary ("Tempos: #{value(item.tempos)}, IDs: #{value(item.trackIDs[0])}")
            }
          }
        }
      }
    }
  }
}
