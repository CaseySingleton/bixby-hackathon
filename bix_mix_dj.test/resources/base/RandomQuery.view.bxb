result-view {
  match { Tracks (tracks) }
  render {
    layout {
      section {
        content {
          paragraph {
            style (Title_M)
            value("Track IDs: #{value(tracks.trackIDs)}")
          }
          paragraph {
            style (Title_M)
            value("Names: #{value(tracks.trackNames)}")
          }
          paragraph {
            style (Title_M)
            value("Tempos: #{value(tracks.tempos)}")
          }
        }
      }
    }
  }
}